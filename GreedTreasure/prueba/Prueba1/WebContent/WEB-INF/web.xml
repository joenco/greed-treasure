<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	
	<display-name>Prueba1</display-name>
	
	<!--
		********************************************************************************
		********************************************************************************
	-->

	<servlet>
		<servlet-name>DynImagesAppServlet</servlet-name>
		<servlet-class>com.tutorial.imagesdyn.DynImagesAppServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DynImagesAppServlet</servlet-name>
		<url-pattern>/imagesdynapp.echo</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>DynImagesDataServlet</servlet-name>
		<servlet-class>com.tutorial.imagesdyn.DynImagesDataServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DynImagesDataServlet</servlet-name>
		<url-pattern>/imagesdyn</url-pattern>
	</servlet-mapping>


	<!--
		********************************************************************************
		********************************************************************************
	-->

</web-app>
