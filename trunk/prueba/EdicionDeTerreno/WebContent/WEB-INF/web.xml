<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">

	<display-name>EdicionDeTerreno</display-name>
	
	<servlet>
		<servlet-name>DynTerrenoAppServlet</servlet-name>
		<servlet-class>com.thinkinglooking.terreno.DynTerrenoAppServlet</servlet-class>
		
	</servlet>

	<servlet-mapping>
		<servlet-name>DynTerrenoAppServlet</servlet-name>
		<url-pattern>/terrenodynapp.echo</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>DynTerrenoDataServlet</servlet-name>
		<servlet-class>com.thinkinglooking.terreno.DynTerrenoDataServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DynTerrenoDataServlet</servlet-name>
		<url-pattern>/terrenodyn</url-pattern>
	</servlet-mapping>
	
</web-app>
